/* pages/score_board/score_board.wxss */
.score_history_top {
  padding: 1px;
  background-color:black;
  color: white;
  border-radius: 0px 10px 0px 0px;
}

.score_history_bottom {
  padding: 1px;
  background-color: black;
  color: white;
  border-radius: 0px 0px 10px 0px;
}

.score_history_hidden {
  padding: 1px;
  background-color: white;
  color: white;
}

.score_current_top {
  padding: 1px;
  border: 1px solid;
  border-radius: 0px 10px 0px 0px;
}

.score_current_bottom {
  padding: 1px;
  border: 1px solid;
  border-radius: 0px 0px 10px 0px;
}

.tool_button {
  width:40px;
  height:40px;
  border: 1px solid;
  margin: 2px;
  border-radius: 5px 5px 5px 5px;
}

.border_black {
  border: 1px solid black;
}

.border_red {
  border: 1px solid red;
}

.share_button {
  width:40px;
  height:40px;
  margin: 2px;
  background-size: 38px 38px;
  background-repeat:no-repeat;
  border-radius: 5px 5px 5px 5px;
  background-image:url('data:image/png;base64,');
}